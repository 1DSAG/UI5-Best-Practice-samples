{
	"_version": "1.12.0",

	"sap.app": {
		"id": "ui5.typescript.helloworld",
		"type": "application",
		"title": "UI5 TypeScript Hello World",
		"description": "UI5 TypeScript Hello World",
		"i18n": {
            "bundleUrl": "i18n/i18n.properties",
            "supportedLocales": ["en", "de"],
            "fallbackLocale": "en"
        },
		"applicationVersion": {
			"version": "1.0.0"
		},
		"ach": "set-ach",
		"dataSources": {
			"Northwind.svc": {
				"uri": "/V2/(S(mo4mpsdbi0fygmzhfv5ik5gf))/OData/OData.svc/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0",
					"localUri": "localService/metadata.xml"
				}
			}
		}
	},

	"sap.fiori": {

	},

	"sap.ui": {
		"technology": "UI5",
		"icons": {

		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},

	"sap.ui5": {
		"rootView": {
			"viewName": "ui5.typescript.helloworld.view.App",
			"type": "XML",
			"async": true,
			"id": "app"
		},

		"dependencies": {
			"minUI5Version": "1.91.0",
			"libs": {
				"sap.ui.core": {},
				"sap.ui.layout": {},
				"sap.ui.unified": {},
				"sap.m": {}
			}
		},

		"handleValidation": true,

		"contentDensities": {
			"compact": true,
			"cozy": true
		},

		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
				 "bundleUrl": "i18n/i18n.properties",
				 "supportedLocales": ["en", "de"],
				 "fallbackLocale": "en"
				}
			},
			"": {
				"dataSource": "Northwind.svc",
				"type": "sap.ui.model.odata.v2.ODataModel",
				"settings": {
					"defaultOperationMode": "Server",
					"defaultBindingMode": "TwoWay",
					"defaultCountMode": "None"
				},
				"preload": false
			}
		},

		"routing": {
			
		}
	}
}